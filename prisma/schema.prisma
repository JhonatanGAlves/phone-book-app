generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Contact {
  id           String @id @default(uuid())
  first_name   String @db.VarChar(50)
  last_name    String @db.VarChar(50)
  phone_number String @db.VarChar(15)
}
